{
  "version": 2,
  "builds": [
    {
      "src": "electrical-event/package.json",
      "use": "@vercel/astro"
    },
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "electrical-event"
    }
  ]
}