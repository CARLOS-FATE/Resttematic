{
  "version": 2,
  "builds": [
    {
      "src": "electrical-event/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/electrical-event/dist/server/entry.mjs"
    }
  ]
}